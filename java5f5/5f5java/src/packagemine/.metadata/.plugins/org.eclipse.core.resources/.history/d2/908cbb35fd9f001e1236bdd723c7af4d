package newproject;
import java.util.*;

public class Practise {
    int x = 10;
    int y = 20;
    int s;
    // Constructor
    public Practise() {
        // You can initialize variables or perform other actions here if needed.
    	s=x+y;
    }

    public static void main(String[] args) {
        Practise obj1 = new Practise();
        System.out.println(obj1.x);
    }
}
