package newproject;
public  class Practise{
	int Sum(int x,int y,int z) {
		return x+y+z;
	}
	String Sum(int x,int y) {
		return x+y;
	}
	public static void main(String args[]) {
		Practise obj1=new Practise();
		int x=20;
		int y=20;
		int z=20;
		System.out.println(obj1.Sum(x, y, z));
		System.out.println(obj1.Sum(x, y));
	
	}
}