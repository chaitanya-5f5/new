package newproject;
import java.util.*;
public class Practise{
	void Sum(int x,int y) {
		return  x+y;
	}
	void Substract(int x,int y) {
		return x-y;
	}
	public static void main(String args[]) {
		int x,y;
		Practise obj1=new Practise(20,3);
		System.out.println("the sum of the two numbers is "+ obj1(20,10));
		System.out.println("the difference of the two numbers is " + obj1(20,9));
	}
}