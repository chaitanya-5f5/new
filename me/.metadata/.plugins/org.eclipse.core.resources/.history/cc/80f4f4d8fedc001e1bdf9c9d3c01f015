package pr1;

public class Arr1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=5,i,j;
		int m=4;
		for(i=0;i<n;i++) {
			for(j=0;j<i;j++) {
				System.out.print("*");
			}
			System.out.println(" ");
		}
		System.out.println();
		for(i=n;i>0;i--) {
			for(j=i;j>0;j--) {
				System.out.print("*");
			}
			System.out.println(" ");
		}
		System.out.println();
		
			
	

}
}
